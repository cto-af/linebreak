  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "repository": "cto-af/linebreak",
  "license": "MIT",
  "devDependencies": {
    "@peggyjs/eslint-config": "4.0.3",
    "@types/node": "22.0.2",
    "c8": "10.1.2",
    "eslint": "9.8.0",
    "eslint-plugin-mocha": "10.5.0",
    "mocha": "10.7.0",
    "rimraf": "6.0.1",
    "typedoc": "0.26.5",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@cto.af/unicode-trie": "2.0.1"
  },
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": ">=18"
  }
